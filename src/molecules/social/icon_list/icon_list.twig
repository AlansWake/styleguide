{% set iconListVariant = iconList.variant | default('osg-v-default') %}
{% set iconListModifiers = iconList.modifiers | default(null) %}

<div class="osg-icons {{ iconListVariant }} {{ iconListModifiers }}">
  {% set brands = iconList.brands %}

  {% for item in brands %}
    {% include 'atoms/social/icon/icon.twig' with {icon: {brand: item.icon, text: item.text, textVisible: item.textVisible, url: item.url, title: item.text }} %}
  {% endfor %}
</div>
