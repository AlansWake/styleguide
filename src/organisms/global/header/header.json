  "searchBarTextField": {
    "name": "textfield",
    "placeholder": "Lorem ipsum dolor sit amet.",
    "id": "searchbar",
    "modifiers": "osg-text-field--outline osg-text-field--yellow"
  },

  "navbarMenuServices": {
    "menuButton": {
      "text": "Meny",
      "textClose": "Meny",
      "color": "blue-dark",
      "modifiers": "osg-hamburger-button osg-button--outline",
      "dataAttrs": [
        {
          "key": "aria-expanded",
          "val": "false"
        }
      ]
    }
  },

  "linkButton": {
    "dataAttrs": [
      {
        "key": "id",
        "val": "lorem_ipsum"
      },
      {
        "key": "aria-label",
        "val": "press me"
      }
    ],
    "url": "#lorem",
    "modifiers": "osg-link-button--outline osg-link-button--yellow",
    "title": "Kontakt"
  },

  "myPage": {
    "title": "Min side",
